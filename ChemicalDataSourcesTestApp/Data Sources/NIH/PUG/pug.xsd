<!--
 (Input) Get a score matrix for a set of IDs; the result is a URL
 to a file containing uid-uid-score triplets.
-->
<xs:element name="PCT-QueryScoreMatrix">
  <xs:complexType>
    <xs:sequence>
<!-- ID list; if ids-to not present, matrix will be ids X ids -->
      <xs:element name="PCT-QueryScoreMatrix_ids">
        <xs:complexType>
          <xs:sequence>
            <xs:element ref="PCT-QueryUids"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
<!-- If given, matrix will be ids X ids-to -->
      <xs:element name="PCT-QueryScoreMatrix_ids-to" minOccurs="0">
        <xs:complexType>
          <xs:sequence>
            <xs:element ref="PCT-QueryUids"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="PCT-QueryScoreMatrix_type">
        <xs:complexType>
          <xs:attribute name="value" use="required">
            <xs:simpleType>
              <xs:restriction base="xs:string">
<!--
    similarity-2d-subs	-  2D Tanimoto similarity based on substructure key fingerprint
    shape-optimized-3d	-  3D similarity optimized by shape
    feature-optimized-3d	-  3D similarity optimized by feature
-->
                <xs:enumeration value="similarity-2d-subs"/>
                <xs:enumeration value="shape-optimized-3d"/>
                <xs:enumeration value="feature-optimized-3d"/>
              </xs:restriction>
            </xs:simpleType>
          </xs:attribute>
        </xs:complexType>
      </xs:element>
      <xs:element name="PCT-QueryScoreMatrix_format">
        <xs:complexType>
          <xs:attribute name="value" use="required">
            <xs:simpleType>
              <xs:restriction base="xs:string">
<!--
    csv	-  Comma-separated values   
    id-id-score	-  Each line has id [tab] id [tab] score   
-->
                <xs:enumeration value="csv"/>
                <xs:enumeration value="id-id-score"/>
              </xs:restriction>
            </xs:simpleType>
          </xs:attribute>
        </xs:complexType>
      </xs:element>
<!-- Default is gzip        -->
      <xs:element name="PCT-QueryScoreMatrix_compression" minOccurs="0">
        <xs:complexType>
          <xs:attribute name="value" use="required">
            <xs:simpleType>
              <xs:restriction base="xs:string">
<!--
    none	-  no compression   
    gzip	-  gzip (.gz)   
    bzip2	-  bzip2 (.bz2)   
-->
                <xs:enumeration value="none"/>
                <xs:enumeration value="gzip"/>
                <xs:enumeration value="bzip2"/>
              </xs:restriction>
            </xs:simpleType>
          </xs:attribute>
        </xs:complexType>
      </xs:element>
<!--
 if 3D, use up to N conformers for each structure (if conformers are not being
 specified directly); not all records may have this many conformers, and upper limit 
 of N is set by the PubChem 3D system (currently 10)
 default 1
-->
      <xs:element name="PCT-QueryScoreMatrix_n-3d-conformers" type="xs:integer" minOccurs="0"/>
<!--
 by default, the 3D structure of a parent is used for scoring if the requested record has no 3D;
 set this to true to avoid this automatic substitution
 default false
-->
      <xs:element name="PCT-QueryScoreMatrix_no-3d-parent" minOccurs="0">
        <xs:complexType>
          <xs:attribute name="value" use="required">
            <xs:simpleType>
              <xs:restriction base="xs:string">
                <xs:enumeration value="true"/>
                <xs:enumeration value="false"/>
              </xs:restriction>
            </xs:simpleType>
          </xs:attribute>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
</xs:element>

<!--
 (Input) Do a link expansion on the given list of IDs, returning either
 an Entrez history item or list of ids, depending on query options.
-->
<xs:element name="PCT-QueryELink">
  <xs:complexType>
    <xs:sequence>
<!-- starting ID list (and db) -->
      <xs:element name="PCT-QueryELink_ids-from">
        <xs:complexType>
          <xs:sequence>
            <xs:element ref="PCT-QueryUids"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
<!--
 link name, same as used by Entrez/eUtils; link
   type must match to/from database
-->
      <xs:element name="PCT-QueryELink_link-name" type="xs:string"/>
<!-- Entrez database of result IDs -->
      <xs:element name="PCT-QueryELink_db-to" type="xs:string"/>
    </xs:sequence>
  </xs:complexType>
</xs:element>

<!-- {Input} PubChem Compound/Substance Conformer Display -->
<xs:element name="PCT-ViewConformer">
  <xs:complexType>
    <xs:sequence>
<!-- PubChem Compound or Substance  -->
      <xs:element name="PCT-ViewConformer_id">
        <xs:complexType>
          <xs:sequence>
            <xs:element ref="PCT-QueryUids"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
<!-- Which conformer to display, default is 0 (1st conf) -->
      <xs:element name="PCT-ViewConformer_conf-idx" type="xs:integer" minOccurs="0"/>
<!--
 Display style of images, default is rocking motion,
   normal speed, no hydrogens, and 400x400 image
-->
      <xs:element name="PCT-ViewConformer_img-style" minOccurs="0">
        <xs:complexType>
          <xs:sequence>
            <xs:element ref="PCT-3DImgStyle"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
</xs:element>

<!-- {Input} Image Style for PubChem 3D View -->
<xs:element name="PCT-3DImgStyle">
  <xs:complexType>
    <xs:sequence>
      <xs:element name="PCT-3DImgStyle_rotation">
        <xs:complexType>
          <xs:simpleContent>
            <xs:extension base="xs:integer">
              <xs:attribute name="value" use="optional">
                <xs:simpleType>
                  <xs:restriction base="xs:string">
<!--
    default	-  Rocking images
    pos-axially	-  Counter-clockwise axially
    neg-axially	-  Clockwise axially
    pos-equatorially	-  Counter-clockwise equatorially
    neg-equatorially	-  Clockwise equatorially
    pos-azimuthally	-  Counter-clockwise azimuthally
    neg-azimuthally	-  Clockwise azimuthally
-->
                    <xs:enumeration value="default" ncbi:intvalue="0"/>
                    <xs:enumeration value="pos-axially" ncbi:intvalue="1"/>
                    <xs:enumeration value="neg-axially" ncbi:intvalue="2"/>
                    <xs:enumeration value="pos-equatorially" ncbi:intvalue="3"/>
                    <xs:enumeration value="neg-equatorially" ncbi:intvalue="4"/>
                    <xs:enumeration value="pos-azimuthally" ncbi:intvalue="5"/>
                    <xs:enumeration value="neg-azimuthally" ncbi:intvalue="6"/>
                  </xs:restriction>
                </xs:simpleType>
              </xs:attribute>
            </xs:extension>
          </xs:simpleContent>
        </xs:complexType>
      </xs:element>
      <xs:element name="PCT-3DImgStyle_speed">
        <xs:complexType>
          <xs:simpleContent>
            <xs:extension base="xs:integer">
              <xs:attribute name="value" use="optional">
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:enumeration value="stop" ncbi:intvalue="0"/>
                    <xs:enumeration value="slow" ncbi:intvalue="1"/>
                    <xs:enumeration value="normal" ncbi:intvalue="2"/>
                    <xs:enumeration value="fast" ncbi:intvalue="3"/>
                  </xs:restriction>
                </xs:simpleType>
              </xs:attribute>
            </xs:extension>
          </xs:simpleContent>
        </xs:complexType>
      </xs:element>
<!-- Display hydrogens -->
      <xs:element name="PCT-3DImgStyle_hydrogen">
        <xs:complexType>
          <xs:attribute name="value" use="required">
            <xs:simpleType>
              <xs:restriction base="xs:string">
                <xs:enumeration value="true"/>
                <xs:enumeration value="false"/>
              </xs:restriction>
            </xs:simpleType>
          </xs:attribute>
        </xs:complexType>
      </xs:element>
<!-- Image size of X by X, 100 <= X <= 1000 -->
      <xs:element name="PCT-3DImgStyle_size" type="xs:integer"/>
    </xs:sequence>
  </xs:complexType>
</xs:element>

<!-- {Input} PubChem Conformer Alignment View -->
<xs:element name="PCT-ViewConformerAlignment">
  <xs:complexType>
    <xs:sequence>
<!-- Conformer superposition matrix -->
      <xs:element name="PCT-ViewConformerAlignment_matrix" type="xs:hexBinary"/>
<!--
 User-defined referenece, conformer information is 
 from 3D search.
-->
      <xs:element name="PCT-ViewConformerAlignment_input-ref" type="xs:hexBinary" minOccurs="0"/>
<!-- User-defined structure string, forwarded by 3D search. -->
      <xs:element name="PCT-ViewConformerAlignment_input-query" type="xs:string" minOccurs="0"/>
      <xs:element name="PCT-ViewConformerAlignment_filter" minOccurs="0">
        <xs:complexType>
          <xs:sequence>
<!-- Maximum shape score for displayed superposition -->
            <xs:element name="PCT-ViewConformerAlignment_filter_shape-score" type="xs:integer"/>
<!-- Maximum feature score for displayed superposition -->
            <xs:element name="PCT-ViewConformerAlignment_filter_feature-score" type="xs:integer"/>
<!-- Remove self neighboring pair to be displayed  -->
            <xs:element name="PCT-ViewConformerAlignment_filter_rmself">
              <xs:complexType>
                <xs:attribute name="value" use="required">
                  <xs:simpleType>
                    <xs:restriction base="xs:string">
                      <xs:enumeration value="true"/>
                      <xs:enumeration value="false"/>
                    </xs:restriction>
                  </xs:simpleType>
                </xs:attribute>
              </xs:complexType>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="PCT-ViewConformerAlignment_optimization">
        <xs:complexType>
          <xs:simpleContent>
            <xs:extension base="xs:integer">
              <xs:attribute name="value" use="optional">
                <xs:simpleType>
                  <xs:restriction base="xs:string">
<!--
    shape-based	-  Maximize overlap of atoms
    feature-based	-  Maximize overlap of features
-->
                    <xs:enumeration value="shape-based" ncbi:intvalue="0"/>
                    <xs:enumeration value="feature-based" ncbi:intvalue="1"/>
                  </xs:restriction>
                </xs:simpleType>
              </xs:attribute>
            </xs:extension>
          </xs:simpleContent>
        </xs:complexType>
      </xs:element>
      <xs:element name="PCT-ViewConformerAlignment_sort" minOccurs="0">
        <xs:complexType>
          <xs:simpleContent>
            <xs:extension base="xs:integer">
              <xs:attribute name="value" use="optional">
                <xs:simpleType>
                  <xs:restriction base="xs:string">
<!--
    shape-then-feature	-  descending order of shape similarity then feature similarity
    feature-then-shape	-  descending order of feature similarity then shape similarity
    shape-and-feature	-  descending order of combined shape and feature similarity
    shape-only	-  descending order of shape similarity
    feature-only	-  descending order of feature similarity
    numerical-id-order	-  ascending numeric ordering of conformer IDs
    input-order	-  no ordering of results (default)
-->
                    <xs:enumeration value="shape-then-feature" ncbi:intvalue="0"/>
                    <xs:enumeration value="feature-then-shape" ncbi:intvalue="1"/>
                    <xs:enumeration value="shape-and-feature" ncbi:intvalue="2"/>
                    <xs:enumeration value="shape-only" ncbi:intvalue="3"/>
                    <xs:enumeration value="feature-only" ncbi:intvalue="4"/>
                    <xs:enumeration value="numerical-id-order" ncbi:intvalue="7"/>
                    <xs:enumeration value="input-order" ncbi:intvalue="8"/>
                  </xs:restriction>
                </xs:simpleType>
              </xs:attribute>
            </xs:extension>
          </xs:simpleContent>
        </xs:complexType>
      </xs:element>
      <xs:element name="PCT-ViewConformerAlignment_algn">
        <xs:complexType>
          <xs:choice>
<!-- indices of Displayed pair superposition -->
            <xs:element name="PCT-ViewConformerAlignment_algn_pair">
              <xs:complexType>
                <xs:sequence>
                  <xs:element ref="PCT-PairSuperposition"/>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
<!-- indices of Displayed ref-fit superposition -->
            <xs:element name="PCT-ViewConformerAlignment_algn_ref-fits">
              <xs:complexType>
                <xs:sequence>
                  <xs:element ref="PCT-RefFitSuperposition"/>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
          </xs:choice>
        </xs:complexType>
      </xs:element>
<!--
 Display style of images, default is rocking motion,
 normal speed, no hydrogens, and 400x400 image
-->
      <xs:element name="PCT-ViewConformerAlignment_img-style" minOccurs="0">
        <xs:complexType>
          <xs:sequence>
            <xs:element ref="PCT-3DImgStyle"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
</xs:element>

<!-- {Input} Compound Pair and and Conformer Pairs for PubChem Conformer Alignment -->
<xs:element name="PCT-PairSuperposition">
  <xs:complexType>
    <xs:sequence>
<!-- Displayed compound pair index -->
      <xs:element name="PCT-PairSuperposition_cmpd-pair-idx" type="xs:integer"/>
<!-- Displayed conformer superposution index -->
      <xs:element name="PCT-PairSuperposition_confpair-idx" type="xs:integer"/>
    </xs:sequence>
  </xs:complexType>
</xs:element>

<!-- {Input} Reference and Fit Conformers for PubChem Conformer Alignment -->
<xs:element name="PCT-RefFitSuperposition">
  <xs:complexType>
    <xs:sequence>
<!-- Displayed ref index     -->
      <xs:element name="PCT-RefFitSuperposition_ref-idx" type="xs:integer"/>
<!-- Displayed fit index -->
      <xs:element name="PCT-RefFitSuperposition_fit-idx" type="xs:integer"/>
<!-- Displayed superposition index -->
      <xs:element name="PCT-RefFitSuperposition_confpair-idx" type="xs:integer"/>
<!-- Displayed superposition matrix index -->
      <xs:element name="PCT-RefFitSuperposition_algn-idx" type="xs:integer"/>
    </xs:sequence>
  </xs:complexType>
</xs:element>

<!-- {Input/Ouput} General data blob container -->
<xs:element name="PCT-DataBlob">
  <xs:complexType>
    <xs:sequence>
<!-- Binary data -->
      <xs:element name="PCT-DataBlob_data" type="xs:hexBinary"/>
      <xs:element name="PCT-DataBlob_format" minOccurs="0">
        <xs:complexType>
          <xs:simpleContent>
            <xs:extension base="xs:integer">
              <xs:attribute name="value" use="optional">
                <xs:simpleType>
                  <xs:restriction base="xs:string">
<!--
    asnb	-  binary ASN.1
    asnt	-  NCBI text ASN.1
-->
                    <xs:enumeration value="unspecified" ncbi:intvalue="0"/>
                    <xs:enumeration value="asnb" ncbi:intvalue="1"/>
                    <xs:enumeration value="asnt" ncbi:intvalue="2"/>
                    <xs:enumeration value="xml" ncbi:intvalue="3"/>
                    <xs:enumeration value="sdf" ncbi:intvalue="4"/>
                    <xs:enumeration value="csv" ncbi:intvalue="5"/>
                    <xs:enumeration value="text" ncbi:intvalue="6"/>
                    <xs:enumeration value="html" ncbi:intvalue="7"/>
                    <xs:enumeration value="png" ncbi:intvalue="8"/>
                    <xs:enumeration value="other" ncbi:intvalue="255"/>
                  </xs:restriction>
                </xs:simpleType>
              </xs:attribute>
            </xs:extension>
          </xs:simpleContent>
        </xs:complexType>
      </xs:element>
      <xs:element name="PCT-DataBlob_compression" minOccurs="0">
        <xs:complexType>
          <xs:attribute name="value" use="required">
            <xs:simpleType>
              <xs:restriction base="xs:string">
<!--
    none	-  default, if unspecified
-->
                <xs:enumeration value="none"/>
                <xs:enumeration value="gzip"/>
                <xs:enumeration value="bzip2"/>
              </xs:restriction>
            </xs:simpleType>
          </xs:attribute>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
</xs:element>

<!-- {Input} ID exchange service options -->
<xs:element name="PCT-QueryIDExchange">
  <xs:complexType>
    <xs:sequence>
<!-- Input IDs (exact match lookup performed) -->
      <xs:element name="PCT-QueryIDExchange_input">
        <xs:complexType>
          <xs:sequence>
            <xs:element ref="PCT-QueryUids"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="PCT-QueryIDExchange_operation-type">
        <xs:complexType>
          <xs:attribute name="value" use="required">
            <xs:simpleType>
              <xs:restriction base="xs:string">
<!--
    same	-  same chemical structure
    sameste	-  same stereochemistry
    sameiso	-  same isotopes
    samecon	-  same connectivity
    samepar	-  same parent
    samepste	-  same parent stereochemistry
    samepiso	-  same parent isotopes
    samepcon	-  same parent connectivity
    similar-compound	-  similar 2D structure
    similar-conformer	-  similar 3D structure
    parent	-  parent compound
-->
                <xs:enumeration value="same"/>
                <xs:enumeration value="sameste"/>
                <xs:enumeration value="sameiso"/>
                <xs:enumeration value="samecon"/>
                <xs:enumeration value="samepar"/>
                <xs:enumeration value="samepste"/>
                <xs:enumeration value="samepiso"/>
                <xs:enumeration value="samepcon"/>
                <xs:enumeration value="similar-compound"/>
                <xs:enumeration value="similar-conformer"/>
                <xs:enumeration value="parent"/>
              </xs:restriction>
            </xs:simpleType>
          </xs:attribute>
        </xs:complexType>
      </xs:element>
      <xs:element name="PCT-QueryIDExchange_output-type">
        <xs:complexType>
          <xs:attribute name="value" use="required">
            <xs:simpleType>
              <xs:restriction base="xs:string">
<!--
    regid	-  Registry IDs
    sid	-  SIDs
    cid	-  CIDs
    inchi	-  InChIs
    inchikey	-  InChIKeys
    synonyms	-  Synonyms (filtered)
-->
                <xs:enumeration value="regid"/>
                <xs:enumeration value="sid"/>
                <xs:enumeration value="cid"/>
                <xs:enumeration value="inchi"/>
                <xs:enumeration value="inchikey"/>
                <xs:enumeration value="synonyms"/>
              </xs:restriction>
            </xs:simpleType>
          </xs:attribute>
        </xs:complexType>
      </xs:element>
<!--
 Data source name to restrict output
   Used only if output_type is (1)
-->
      <xs:element name="PCT-QueryIDExchange_output-dsn" type="xs:string" minOccurs="0"/>
      <xs:element name="PCT-QueryIDExchange_output-method">
        <xs:complexType>
          <xs:attribute name="value" use="required">
            <xs:simpleType>
              <xs:restriction base="xs:string">
<!--
    entrez	-  Provide an Entrez History (Substance or Compound only)
    file-list	-  Single column file showing unique results
    file-pair	-  Two column file showing each input-output correspondence
-->
                <xs:enumeration value="entrez"/>
                <xs:enumeration value="file-list"/>
                <xs:enumeration value="file-pair"/>
              </xs:restriction>
            </xs:simpleType>
          </xs:attribute>
        </xs:complexType>
      </xs:element>
<!--
 Destination WebEnvironment to put history
   Used only if output_method is (1)
-->
      <xs:element name="PCT-QueryIDExchange_webenv" type="xs:string" minOccurs="0"/>
<!-- Default is gzip -->
      <xs:element name="PCT-QueryIDExchange_compression" minOccurs="0">
        <xs:complexType>
          <xs:attribute name="value" use="required">
            <xs:simpleType>
              <xs:restriction base="xs:string">
<!--
    none	-  no compression
    gzip	-  gzip (.gz)
    bzip2	-  bzip2 (.bz2)
-->
                <xs:enumeration value="none"/>
                <xs:enumeration value="gzip"/>
                <xs:enumeration value="bzip2"/>
              </xs:restriction>
            </xs:simpleType>
          </xs:attribute>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
</xs:element>

<!-- {Input} 3D service options -->
<xs:element name="PCT-QueryPC3D">
  <xs:complexType>
    <xs:sequence>
<!-- ids to align - currently CIDs only -->
      <xs:element name="PCT-QueryPC3D_ids">
        <xs:complexType>
          <xs:sequence>
            <xs:element ref="PCT-QueryUids"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
</xs:element>

</xs:schema>
