                </xs:simpleType>
              </xs:attribute>
            </xs:extension>
          </xs:simpleContent>
        </xs:complexType>
      </xs:element>
      <xs:element name="PCT-Assay-FocusOption_duplicate-test-option" minOccurs="0">
        <xs:complexType>
          <xs:simpleContent>
            <xs:extension base="xs:integer">
              <xs:attribute name="value" use="optional">
                <xs:simpleType>
                  <xs:restriction base="xs:string">
<!--
    flag-discrepencies	-  Flag Discrepancies      (default, if not specified)
    exclude-duplicate	-  Exclude Duplicates
    most-recent	-  Most Recent
    most-active	-  Most Active
    least-active	-  Least Active
    show-discrepant-only	-  Show discrepant only
-->
                    <xs:enumeration value="flag-discrepencies" ncbi:intvalue="0"/>
                    <xs:enumeration value="exclude-duplicate" ncbi:intvalue="1"/>
                    <xs:enumeration value="most-recent" ncbi:intvalue="2"/>
                    <xs:enumeration value="most-active" ncbi:intvalue="3"/>
                    <xs:enumeration value="least-active" ncbi:intvalue="4"/>
                    <xs:enumeration value="show-discrepant-only" ncbi:intvalue="5"/>
                  </xs:restriction>
                </xs:simpleType>
              </xs:attribute>
            </xs:extension>
          </xs:simpleContent>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
