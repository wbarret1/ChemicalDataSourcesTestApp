      <xs:element name="PCT-Standardize_structure">
        <xs:complexType>
          <xs:sequence>
            <xs:element ref="PCT-Structure"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
<!-- structure output format -->
      <xs:element name="PCT-Standardize_oformat">
        <xs:complexType>
          <xs:sequence>
            <xs:element ref="PCT-StructureFormat"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
