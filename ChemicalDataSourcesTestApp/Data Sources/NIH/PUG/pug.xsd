<!-- {Input} Query request -->
<xs:element name="PCT-Query">
  <xs:complexType>
    <xs:sequence>
<!-- query to be performed -->
      <xs:element name="PCT-Query_type">
        <xs:complexType>
          <xs:sequence minOccurs="0" maxOccurs="unbounded">
            <xs:element ref="PCT-QueryType"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
<!-- id subset to query against, entire DB used if empty -->
      <xs:element name="PCT-Query_subset" minOccurs="0">
        <xs:complexType>
          <xs:sequence>
            <xs:element ref="PCT-QueryUids"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="PCT-Query_options" minOccurs="0">
        <xs:complexType>
          <xs:choice>
<!--
 result id list should be inserted into this history;
 response is PCT-OutputData.output.entrez (with new query key)
-->
            <xs:element name="PCT-Query_options_webenv" type="xs:string"/>
<!-- return at most this # ids (or all if zero), as PCT-OutputData.output.ids -->
            <xs:element name="PCT-Query_options_ids" type="xs:integer"/>
          </xs:choice>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
</xs:element>

