<!-- {Input} PubChem BioAssay data focus options -->
<xs:element name="PCT-Assay-FocusOption">
  <xs:complexType>
    <xs:sequence>
      <xs:element name="PCT-Assay-FocusOption_group-results-by" minOccurs="0">
        <xs:complexType>
          <xs:simpleContent>
            <xs:extension base="xs:integer">
              <xs:attribute name="value" use="optional">
                <xs:simpleType>
                  <xs:restriction base="xs:string">
<!--
    compound	-  Compound            (default, if not specified)
    compound-same-connectivity	-  Compound, Same Connectivity
    parent	-  Parent Compound
    parent-same-connectivity	-  Parent Compound, Same Connectivity
    substance	-  Substance
-->
                    <xs:enumeration value="compound" ncbi:intvalue="0"/>
                    <xs:enumeration value="compound-same-connectivity" ncbi:intvalue="1"/>
                    <xs:enumeration value="parent" ncbi:intvalue="2"/>
                    <xs:enumeration value="parent-same-connectivity" ncbi:intvalue="3"/>
                    <xs:enumeration value="substance" ncbi:intvalue="4"/>
