<!-- Assay Revision             (defaults to current) -->
      <xs:element name="PCT-Assay-Readout_revision" type="xs:integer" minOccurs="0"/>
<!-- Assay Data Readouts (TIDs) (defaults to all data) -->
      <xs:element name="PCT-Assay-Readout_retrieve" minOccurs="0">
        <xs:complexType>
          <xs:sequence minOccurs="0" maxOccurs="unbounded">
            <xs:element name="PCT-Assay-Readout_retrieve_E" type="xs:integer"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
<!-- Filter Assay Data by Readout -->
      <xs:element name="PCT-Assay-Readout_query" minOccurs="0">
        <xs:complexType>
          <xs:sequence minOccurs="0" maxOccurs="unbounded">
            <xs:element ref="PCT-Assay-Readout-Query"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="PCT-Assay-Readout_result-version" minOccurs="0">
        <xs:complexType>
          <xs:simpleContent>
            <xs:extension base="xs:integer">
              <xs:attribute name="value" use="optional">
                <xs:simpleType>
                  <xs:restriction base="xs:string">
<!--
    current	-  Current Assay Test Result                 (default, if not specified)
    allversion	-  All Assay Result Versions         (only for single assay search/view)
-->
