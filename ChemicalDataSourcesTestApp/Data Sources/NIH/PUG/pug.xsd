      <xs:element name="PCT-Waiting_reqid" type="xs:string"/>
<!-- qualifying message regarding new request -->
      <xs:element name="PCT-Waiting_message" type="xs:string" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>
</xs:element>

<!-- {Input} Download data -->
<xs:element name="PCT-Download">
  <xs:complexType>
    <xs:sequence>
<!-- record identifiers to download -->
      <xs:element name="PCT-Download_uids">
        <xs:complexType>
          <xs:sequence>
            <xs:element ref="PCT-QueryUids"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="PCT-Download_format">
        <xs:complexType>
          <xs:attribute name="value" use="required">
            <xs:simpleType>
              <xs:restriction base="xs:string">
<!--
    text-asn	-  full records
         textual ASN.1
    binary-asn	-  binary ASN.1
    xml	-  textual XML
    sdf	-  SD file format, for chemical structures
    image	-  images, format is always .zip containing multiple .png
         full-size depiction
    image-small	-  thumbnail depiction
    smiles	-  selected string fields, format is: SID/CID <tab> <string>
         Isomeric SMILES
    inchi	-  InChI
-->
                <xs:enumeration value="text-asn"/>
                <xs:enumeration value="binary-asn"/>
                <xs:enumeration value="xml"/>
                <xs:enumeration value="sdf"/>
                <xs:enumeration value="image"/>
                <xs:enumeration value="image-small"/>
                <xs:enumeration value="smiles"/>
                <xs:enumeration value="inchi"/>
              </xs:restriction>
            </xs:simpleType>
          </xs:attribute>
        </xs:complexType>
      </xs:element>
<!-- default gzip -->
      <xs:element name="PCT-Download_compression" minOccurs="0">
        <xs:complexType>
          <xs:attribute name="value" use="required">
            <xs:simpleType>
              <xs:restriction base="xs:string">
<!--
    none	-  no compression
    gzip	-  gzip format
    bzip2	-  bzip2 format
-->
                <xs:enumeration value="none"/>
                <xs:enumeration value="gzip"/>
                <xs:enumeration value="bzip2"/>
              </xs:restriction>
            </xs:simpleType>
          </xs:attribute>
        </xs:complexType>
      </xs:element>
<!--
 whether to retrieve 3D records (when applicable) instead of 2D
 default false (2D)
-->
      <xs:element name="PCT-Download_use-3d" minOccurs="0">
        <xs:complexType>
          <xs:attribute name="value" use="required">
            <xs:simpleType>
              <xs:restriction base="xs:string">
                <xs:enumeration value="true"/>
                <xs:enumeration value="false"/>
              </xs:restriction>
            </xs:simpleType>
          </xs:attribute>
        </xs:complexType>
      </xs:element>
<!--
 if 3D, return up to N conformers for each structure (if conformers are not being
 specified directly); not all records may have this many conformers, and upper limit 
 of N is set by the PubChem 3D system (currently 10)
 default 1
-->
      <xs:element name="PCT-Download_n-3d-conformers" type="xs:integer" minOccurs="0"/>
<!--
 if a single UID is provided, optionally retrieve that record synchronously in the
 response object, without going through the usual waiting cycle and URL download
 default false
-->
      <xs:element name="PCT-Download_get-single-record" minOccurs="0">
        <xs:complexType>
          <xs:attribute name="value" use="required">
            <xs:simpleType>
              <xs:restriction base="xs:string">
                <xs:enumeration value="true"/>
                <xs:enumeration value="false"/>
              </xs:restriction>
            </xs:simpleType>
          </xs:attribute>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
</xs:element>

<!-- {Output} Results are available by using the provided URL -->
<xs:element name="PCT-Download-URL">
  <xs:complexType>
    <xs:sequence>
<!-- location of the result -->
      <xs:element name="PCT-Download-URL_url" type="xs:string"/>
<!-- qualifying message -->
      <xs:element name="PCT-Download-URL_message" type="xs:string" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>
</xs:element>

