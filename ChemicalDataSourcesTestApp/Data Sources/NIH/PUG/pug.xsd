      <xs:element name="PCT-Data_output">
        <xs:complexType>
          <xs:sequence>
            <xs:element ref="PCT-OutputData"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:choice>
  </xs:complexType>
</xs:element>

