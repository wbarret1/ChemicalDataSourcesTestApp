<!-- {Input} PubChem BioAssay Data Query -->
<xs:element name="PCT-QueryAssayData">
  <xs:complexType>
    <xs:sequence>
      <xs:element name="PCT-QueryAssayData_output">
        <xs:complexType>
          <xs:simpleContent>
            <xs:extension base="xs:integer">
              <xs:attribute name="value" use="optional">
                <xs:simpleType>
                  <xs:restriction base="xs:string">
<!--
    assay-xml	-  XML format using PubChem BioAssay data specification
    assay-text-asn	-  text ASN.1 format using PubChem BioAssay data specification
    assay-binary-asn	-  binary ASN.1 format using PubChem BioAssay data specification
    csv	-  CSV data format
-->
                    <xs:enumeration value="assay-xml" ncbi:intvalue="1"/>
                    <xs:enumeration value="assay-text-asn" ncbi:intvalue="2"/>
                    <xs:enumeration value="assay-binary-asn" ncbi:intvalue="3"/>
                    <xs:enumeration value="csv" ncbi:intvalue="4"/>
                  </xs:restriction>
                </xs:simpleType>
              </xs:attribute>
            </xs:extension>
          </xs:simpleContent>
        </xs:complexType>
      </xs:element>
<!-- AIDs to retrieve -->
      <xs:element name="PCT-QueryAssayData_aids">
        <xs:complexType>
          <xs:sequence>
            <xs:element ref="PCT-QueryUids"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
<!-- Restrict rows to these SIDs or CIDs -->
      <xs:element name="PCT-QueryAssayData_scids" minOccurs="0">
        <xs:complexType>
          <xs:sequence>
            <xs:element ref="PCT-QueryUids"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="PCT-QueryAssayData_dataset" minOccurs="0">
        <xs:complexType>
          <xs:simpleContent>
            <xs:extension base="xs:integer">
              <xs:attribute name="value" use="optional">
                <xs:simpleType>
                  <xs:restriction base="xs:string">
<!--
    complete	-  detail view
    concise	-  summary view
-->
                    <xs:enumeration value="complete" ncbi:intvalue="0"/>
                    <xs:enumeration value="concise" ncbi:intvalue="1"/>
                  </xs:restriction>
                </xs:simpleType>
              </xs:attribute>
            </xs:extension>
          </xs:simpleContent>
        </xs:complexType>
      </xs:element>
<!-- Query/restrict bioassay readouts -->
      <xs:element name="PCT-QueryAssayData_readouts" minOccurs="0">
        <xs:complexType>
          <xs:sequence minOccurs="0" maxOccurs="unbounded">
            <xs:element ref="PCT-Assay-Readout"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
<!-- Data focus options -->
      <xs:element name="PCT-QueryAssayData_focus" minOccurs="0">
        <xs:complexType>
          <xs:sequence>
            <xs:element ref="PCT-Assay-FocusOption"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
<!-- default gzip -->
      <xs:element name="PCT-QueryAssayData_compression" minOccurs="0">
        <xs:complexType>
          <xs:attribute name="value" use="required">
            <xs:simpleType>
              <xs:restriction base="xs:string">
<!--
    none	-  no compression
    gzip	-  gzip (.gz) format
    bzip2	-  bzip2 (.bz2) format
-->
                <xs:enumeration value="none"/>
                <xs:enumeration value="gzip"/>
                <xs:enumeration value="bzip2"/>
              </xs:restriction>
            </xs:simpleType>
          </xs:attribute>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
</xs:element>

