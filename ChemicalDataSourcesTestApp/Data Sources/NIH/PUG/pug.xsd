<!-- (Input/Output) Single PubChem Structure -->
<xs:element name="PCT-Structure">
  <xs:complexType>
    <xs:sequence>
      <xs:element name="PCT-Structure_structure">
        <xs:complexType>
          <xs:choice>
<!-- Input is a file -->
            <xs:element name="PCT-Structure_structure_binary" type="xs:hexBinary"/>
<!-- Single-line text -->
            <xs:element name="PCT-Structure_structure_string" type="xs:string"/>
<!-- Multi-line text -->
            <xs:element name="PCT-Structure_structure_strings">
              <xs:complexType>
                <xs:sequence minOccurs="0" maxOccurs="unbounded">
                  <xs:element name="PCT-Structure_structure_strings_E" type="xs:string"/>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
          </xs:choice>
        </xs:complexType>
      </xs:element>
<!-- chemical structure format -->
      <xs:element name="PCT-Structure_format">
        <xs:complexType>
          <xs:sequence>
            <xs:element ref="PCT-StructureFormat"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
<!-- Default is no compression -->
      <xs:element name="PCT-Structure_compression" minOccurs="0">
        <xs:complexType>
          <xs:attribute name="value" use="required">
            <xs:simpleType>
              <xs:restriction base="xs:string">
<!--
    none	-  no compression
    gzip	-  gzip format
    bzip2	-  bzip2 format
-->
                <xs:enumeration value="none"/>
                <xs:enumeration value="gzip"/>
                <xs:enumeration value="bzip2"/>
              </xs:restriction>
            </xs:simpleType>
          </xs:attribute>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
</xs:element>

<!-- (Input/Output) PubChem Structure Format -->
<xs:element name="PCT-StructureFormat">
  <xs:complexType>
    <xs:attribute name="value" use="required">
      <xs:simpleType>
        <xs:restriction base="xs:string">
<!--
    asnb	-  PubChem ASN.1 Binary Format
    asnt	-  PubChem ASN.1 Text Format
    xml	-  PubChem XML Format
    sdf	-  SDF Format
    smiles	-  SMILES Format
    inchi	-  InChI Format
-->
          <xs:enumeration value="asnb"/>
          <xs:enumeration value="asnt"/>
          <xs:enumeration value="xml"/>
          <xs:enumeration value="sdf"/>
          <xs:enumeration value="smiles"/>
          <xs:enumeration value="inchi"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:complexType>
</xs:element>

