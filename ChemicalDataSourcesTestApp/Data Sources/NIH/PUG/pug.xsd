<!-- External Registry IDs -->
      <xs:element name="PCT-RegistryIDs_source-ids">
        <xs:complexType>
          <xs:sequence minOccurs="0" maxOccurs="unbounded">
            <xs:element name="PCT-RegistryIDs_source-ids_E" type="xs:string"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
</xs:element>

<!-- {Input, Output} Entrez (history) search description -->
<xs:element name="PCT-Entrez">
  <xs:complexType>
    <xs:sequence>
<!-- Entrez database identifier -->
      <xs:element name="PCT-Entrez_db" type="xs:string"/>
<!-- Entrez history search query key -->
      <xs:element name="PCT-Entrez_query-key" type="xs:string" minOccurs="0"/>
<!-- Entrez history web environment -->
      <xs:element name="PCT-Entrez_webenv" type="xs:string" minOccurs="0"/>
<!-- Entrez history record count -->
      <xs:element name="PCT-Entrez_count" type="xs:string" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>
</xs:element>

<!-- {Input} PubChem Compound chemical structure query description -->
<xs:element name="PCT-QueryCompoundCS">
  <xs:complexType>
    <xs:sequence>
      <xs:element name="PCT-QueryCompoundCS_query">
        <xs:complexType>
          <xs:choice>
<!-- query is a file -->
            <xs:element name="PCT-QueryCompoundCS_query_file" type="xs:hexBinary"/>
<!-- query is a string -->
            <xs:element name="PCT-QueryCompoundCS_query_data" type="xs:string"/>
<!-- multiple queries as strings, one per line -->
            <xs:element name="PCT-QueryCompoundCS_query_multi">
              <xs:complexType>
                <xs:sequence minOccurs="0" maxOccurs="unbounded">
                  <xs:element name="PCT-QueryCompoundCS_query_multi_E" type="xs:string"/>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
          </xs:choice>
        </xs:complexType>
      </xs:element>
      <xs:element name="PCT-QueryCompoundCS_type">
        <xs:complexType>
          <xs:choice>
<!-- structure identity query -->
            <xs:element name="PCT-QueryCompoundCS_type_identical">
              <xs:complexType>
                <xs:sequence>
                  <xs:element ref="PCT-CSIdentity"/>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
<!-- substructure query -->
            <xs:element name="PCT-QueryCompoundCS_type_subss">
              <xs:complexType>
                <xs:sequence>
                  <xs:element ref="PCT-CSStructure"/>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
<!-- superstructure query -->
            <xs:element name="PCT-QueryCompoundCS_type_supss">
              <xs:complexType>
                <xs:sequence>
                  <xs:element ref="PCT-CSStructure"/>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
<!-- similarity search query -->
            <xs:element name="PCT-QueryCompoundCS_type_similar">
              <xs:complexType>
                <xs:sequence>
                  <xs:element ref="PCT-CSSimilarity"/>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
<!-- molecular formula query -->
            <xs:element name="PCT-QueryCompoundCS_type_formula">
              <xs:complexType>
                <xs:sequence>
                  <xs:element ref="PCT-CSMolFormula"/>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
<!-- 3d conformer query -->
            <xs:element name="PCT-QueryCompoundCS_type_conformer">
              <xs:complexType>
                <xs:sequence>
                  <xs:element ref="PCT-SimilarConformer"/>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
          </xs:choice>
        </xs:complexType>
      </xs:element>
<!-- time limit (in CPU seconds) -->
      <xs:element name="PCT-QueryCompoundCS_time" type="xs:integer" minOccurs="0"/>
<!-- hit limit count -->
      <xs:element name="PCT-QueryCompoundCS_results" type="xs:integer" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>
</xs:element>

<!-- {Input} PubChem Compound chemical structure identity search options -->
<xs:element name="PCT-CSIdentity">
  <xs:complexType>
    <xs:simpleContent>
      <xs:extension base="xs:integer">
        <xs:attribute name="value" use="optional">
          <xs:simpleType>
            <xs:restriction base="xs:string">
<!--
    same-connectivity	-  must have same connectivity
    any-tautomer	-  can be a tautomer of query, ignores stereo and isotopic substitution
    same-stereo	-  must have same connectivity and stereo
    same-isotope	-  must have same connectivity and isotopic substitutions
    same-stereo-isotope	-  must have same connectivity, stereo, and isotopic substitutions
    same-nonconflict-stereo	-  must have same connectivity and non-conflicting stereo
    same-isotope-nonconflict-stereo	-  must have same connectivity, isotopic substitutions and non-conflicting stereo
-->
              <xs:enumeration value="undefined" ncbi:intvalue="0"/>
              <xs:enumeration value="same-connectivity" ncbi:intvalue="1"/>
              <xs:enumeration value="any-tautomer" ncbi:intvalue="2"/>
              <xs:enumeration value="same-stereo" ncbi:intvalue="3"/>
              <xs:enumeration value="same-isotope" ncbi:intvalue="4"/>
              <xs:enumeration value="same-stereo-isotope" ncbi:intvalue="5"/>
              <xs:enumeration value="same-nonconflict-stereo" ncbi:intvalue="6"/>
              <xs:enumeration value="same-isotope-nonconflict-stereo" ncbi:intvalue="7"/>
            </xs:restriction>
          </xs:simpleType>
        </xs:attribute>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>
</xs:element>

<!-- {Input} PubChem Compound chemical structure molecular formula search options -->
<xs:element name="PCT-CSMolFormula">
  <xs:complexType>
    <xs:sequence>
<!-- are elements not specified allowed to be present in the result -->
      <xs:element name="PCT-CSMolFormula_allow-other" minOccurs="0">
        <xs:complexType>
          <xs:attribute name="value" use="required">
            <xs:simpleType>
              <xs:restriction base="xs:string">
                <xs:enumeration value="true"/>
                <xs:enumeration value="false"/>
              </xs:restriction>
            </xs:simpleType>
          </xs:attribute>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
</xs:element>

<!-- {Input} PubChem Compound chemical structure similarity search options -->
<xs:element name="PCT-CSSimilarity">
  <xs:complexType>
    <xs:sequence>
<!-- similarity search Tanimoto threshold -->
      <xs:element name="PCT-CSSimilarity_threshold" type="xs:integer"/>
    </xs:sequence>
  </xs:complexType>
</xs:element>

<!-- {Input} 3d conformer query for similar conformers in PC Compound -->
<xs:element name="PCT-SimilarConformer">
  <xs:complexType>
    <xs:sequence>
      <xs:element name="PCT-SimilarConformer_sort" minOccurs="0">
        <xs:complexType>
          <xs:simpleContent>
            <xs:extension base="xs:integer">
              <xs:attribute name="value" use="optional">
                <xs:simpleType>
                  <xs:restriction base="xs:string">
<!--
    input-order	-  no ordering of results (default)
    shape-then-feature	-  descending order of shape similarity then feature similarity
    feature-then-shape	-  descending order of feature similarity then shape similarity
    shape-and-feature	-  descending order of combined shape and feature similarity
    numerical-id-order	-  ascending numeric ordering of conformer IDs
-->
                    <xs:enumeration value="input-order" ncbi:intvalue="0"/>
                    <xs:enumeration value="shape-then-feature" ncbi:intvalue="1"/>
                    <xs:enumeration value="feature-then-shape" ncbi:intvalue="2"/>
                    <xs:enumeration value="shape-and-feature" ncbi:intvalue="3"/>
                    <xs:enumeration value="numerical-id-order" ncbi:intvalue="4"/>
                  </xs:restriction>
                </xs:simpleType>
              </xs:attribute>
            </xs:extension>
          </xs:simpleContent>
        </xs:complexType>
      </xs:element>
      <xs:element name="PCT-SimilarConformer_output">
        <xs:complexType>
          <xs:simpleContent>
            <xs:extension base="xs:integer">
              <xs:attribute name="value" use="optional">
                <xs:simpleType>
                  <xs:restriction base="xs:string">
<!--
    display	-  result is an URL to display results in PC3D web-viewer
    sdf	-  result is an FTP URL to SDF text file
    asn	-  result is an FTP URL to ASN.1 binary file
    xml	-  result is an FTP URL to XML text file
    csv	-  result is an FTP URL to CSV text file
    entrez	-  result is an Entrez webenv
-->
                    <xs:enumeration value="display" ncbi:intvalue="1"/>
                    <xs:enumeration value="sdf" ncbi:intvalue="2"/>
                    <xs:enumeration value="asn" ncbi:intvalue="3"/>
                    <xs:enumeration value="xml" ncbi:intvalue="4"/>
                    <xs:enumeration value="csv" ncbi:intvalue="5"/>
                    <xs:enumeration value="entrez" ncbi:intvalue="6"/>
                  </xs:restriction>
                </xs:simpleType>
              </xs:attribute>
            </xs:extension>
          </xs:simpleContent>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
</xs:element>

<!-- {Input} PubChem Compound chemical structure substructure and superstructure search options -->
<xs:element name="PCT-CSStructure">
  <xs:complexType>
    <xs:sequence>
      <xs:element name="PCT-CSStructure_stereo" minOccurs="0">
        <xs:complexType>
          <xs:simpleContent>
            <xs:extension base="xs:integer">
              <xs:attribute name="value" use="optional">
                <xs:simpleType>
                  <xs:restriction base="xs:string">
<!--
    ignore	-  ignore stereochemistry (default, if not specified)
    exact	-  match exact stereochemistry
    relative	-  match relative stereochemistry
    non-conflicting	-  match specified or non-specific stereo exactly
-->
                    <xs:enumeration value="ignore" ncbi:intvalue="0"/>
                    <xs:enumeration value="exact" ncbi:intvalue="1"/>
                    <xs:enumeration value="relative" ncbi:intvalue="2"/>
                    <xs:enumeration value="non-conflicting" ncbi:intvalue="3"/>
                  </xs:restriction>
                </xs:simpleType>
              </xs:attribute>
            </xs:extension>
          </xs:simpleContent>
        </xs:complexType>
      </xs:element>
<!-- match isotopes           (default false) -->
      <xs:element name="PCT-CSStructure_isotopes" minOccurs="0">
        <xs:complexType>
          <xs:attribute name="value" use="required">
            <xs:simpleType>
              <xs:restriction base="xs:string">
                <xs:enumeration value="true"/>
                <xs:enumeration value="false"/>
              </xs:restriction>
            </xs:simpleType>
          </xs:attribute>
        </xs:complexType>
      </xs:element>
<!-- match charges            (default false) -->
      <xs:element name="PCT-CSStructure_charges" minOccurs="0">
        <xs:complexType>
          <xs:attribute name="value" use="required">
            <xs:simpleType>
              <xs:restriction base="xs:string">
                <xs:enumeration value="true"/>
                <xs:enumeration value="false"/>
              </xs:restriction>
            </xs:simpleType>
          </xs:attribute>
        </xs:complexType>
      </xs:element>
<!-- match tautomers          (default false) -->
      <xs:element name="PCT-CSStructure_tautomers" minOccurs="0">
        <xs:complexType>
          <xs:attribute name="value" use="required">
            <xs:simpleType>
              <xs:restriction base="xs:string">
                <xs:enumeration value="true"/>
                <xs:enumeration value="false"/>
              </xs:restriction>
            </xs:simpleType>
          </xs:attribute>
        </xs:complexType>
      </xs:element>
<!-- ringsystems not embedded (default false) -->
      <xs:element name="PCT-CSStructure_rings" minOccurs="0">
        <xs:complexType>
          <xs:attribute name="value" use="required">
            <xs:simpleType>
              <xs:restriction base="xs:string">
                <xs:enumeration value="true"/>
                <xs:enumeration value="false"/>
              </xs:restriction>
            </xs:simpleType>
          </xs:attribute>
        </xs:complexType>
      </xs:element>
<!-- singe/double bonds match (default true) -->
      <xs:element name="PCT-CSStructure_bonds" minOccurs="0">
        <xs:complexType>
          <xs:attribute name="value" use="required">
            <xs:simpleType>
              <xs:restriction base="xs:string">
                <xs:enumeration value="true"/>
                <xs:enumeration value="false"/>
              </xs:restriction>
            </xs:simpleType>
          </xs:attribute>
        </xs:complexType>
      </xs:element>
<!-- chain bonds match rings  (default true) -->
      <xs:element name="PCT-CSStructure_chains" minOccurs="0">
        <xs:complexType>
          <xs:attribute name="value" use="required">
            <xs:simpleType>
              <xs:restriction base="xs:string">
                <xs:enumeration value="true"/>
                <xs:enumeration value="false"/>
              </xs:restriction>
            </xs:simpleType>
          </xs:attribute>
        </xs:complexType>
      </xs:element>
<!-- strip hydrogens          (default false) -->
      <xs:element name="PCT-CSStructure_hydrogen" minOccurs="0">
        <xs:complexType>
          <xs:attribute name="value" use="required">
            <xs:simpleType>
              <xs:restriction base="xs:string">
                <xs:enumeration value="true"/>
                <xs:enumeration value="false"/>
              </xs:restriction>
            </xs:simpleType>
          </xs:attribute>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
</xs:element>

<!-- {Input} PubChem Compound Entrez limits page query -->
