<!-- {Input} PubChem Compound Entrez Limits Search -->
<xs:element name="PCT-CLByProperty">
  <xs:complexType>
    <xs:sequence>
      <xs:element name="PCT-CLByProperty_type">
        <xs:complexType>
          <xs:simpleContent>
            <xs:extension base="xs:integer">
              <xs:attribute name="value" use="optional">
                <xs:simpleType>
                  <xs:restriction base="xs:string">
<!--
    mw	-  Molecular Weight                 (FLOAT)
    xlogp	-  xLogP                            (FLOAT)
    hbd	-  Count of Hydrogen Bond Donors    (INTGER)
    hba	-  Count of Hydrogen Bond Acceptors (INTEGER)
    rb	-  Count of Rotatable Bonds         (INTEGER)
    tpsa	-  Topological Polar Surface Area   (FLOAT)
    ha	-  Count of Heavy Atoms             (INTEGER)
    isotope	-  Count of Specified Isotopes      (INTEGER)
    tautomer	-  Count of Tautomers               (INTEGER)
    cu	-  Count of Covalent Units          (INTEGER)
    complexity	-  Structure Complexity             (FLOAT)
    charge	-  Formal Charge                    (INTGER)
-->
                    <xs:enumeration value="mw" ncbi:intvalue="1"/>
                    <xs:enumeration value="xlogp" ncbi:intvalue="2"/>
                    <xs:enumeration value="hbd" ncbi:intvalue="3"/>
                    <xs:enumeration value="hba" ncbi:intvalue="4"/>
                    <xs:enumeration value="rb" ncbi:intvalue="5"/>
                    <xs:enumeration value="tpsa" ncbi:intvalue="6"/>
                    <xs:enumeration value="ha" ncbi:intvalue="7"/>
                    <xs:enumeration value="isotope" ncbi:intvalue="8"/>
                    <xs:enumeration value="tautomer" ncbi:intvalue="9"/>
                    <xs:enumeration value="cu" ncbi:intvalue="10"/>
                    <xs:enumeration value="complexity" ncbi:intvalue="11"/>
                    <xs:enumeration value="charge" ncbi:intvalue="12"/>
                  </xs:restriction>
                </xs:simpleType>
              </xs:attribute>
            </xs:extension>
          </xs:simpleContent>
        </xs:complexType>
      </xs:element>
      <xs:element name="PCT-CLByProperty_range">
        <xs:complexType>
          <xs:choice>
<!-- Floating Point Range Specification -->
            <xs:element name="PCT-CLByProperty_range_frange">
              <xs:complexType>
                <xs:sequence>
                  <xs:element ref="PCT-CLFloatRange"/>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
<!-- Integer Point Range Specification -->
            <xs:element name="PCT-CLByProperty_range_irange">
              <xs:complexType>
                <xs:sequence>
                  <xs:element ref="PCT-CLIntegerRange"/>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
          </xs:choice>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
</xs:element>

