-->
                <xs:enumeration value="unknown"/>
                <xs:enumeration value="status"/>
                <xs:enumeration value="cancel"/>
              </xs:restriction>
            </xs:simpleType>
          </xs:attribute>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
</xs:element>

<!-- {Output} Status message of the task -->
