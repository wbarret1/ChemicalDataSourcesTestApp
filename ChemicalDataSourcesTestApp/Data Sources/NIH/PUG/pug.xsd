          </xs:sequence>
        </xs:complexType>
      </xs:element>
<!-- message qualifying the status -->
      <xs:element name="PCT-Status-Message_message" type="xs:string" minOccurs="0"/>
<!-- messages qualifying the status -->
      <xs:element name="PCT-Status-Message_messages" minOccurs="0">
        <xs:complexType>
          <xs:sequence minOccurs="0" maxOccurs="unbounded">
            <xs:element name="PCT-Status-Message_messages_E" type="xs:string"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
</xs:element>

<!-- {Output} Status of the task -->
<xs:element name="PCT-Status">
  <xs:complexType>
    <xs:attribute name="value" use="required">
      <xs:simpleType>
        <xs:restriction base="xs:string">
<!--
    success	-  task completed successfully
    server-error	-  request completion
         general server failure
    hit-limit	-  success, but hit limit reached
    time-limit	-  success, but time limit reached
    input-error	-  input errors
         problem with input options
    data-error	-  problem with input data
    stopped	-  request management status
         request was stopped
    running	-  request is running
    queued	-  request is queued
-->
          <xs:enumeration value="unknown"/>
          <xs:enumeration value="success"/>
          <xs:enumeration value="server-error"/>
          <xs:enumeration value="hit-limit"/>
          <xs:enumeration value="time-limit"/>
          <xs:enumeration value="input-error"/>
          <xs:enumeration value="data-error"/>
          <xs:enumeration value="stopped"/>
          <xs:enumeration value="running"/>
          <xs:enumeration value="queued"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:complexType>
</xs:element>

